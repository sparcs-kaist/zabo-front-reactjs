{
  "name": "zabo-client-reactjs",
  "description": "This repository is powered by SPARCS KAIST",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "env:path": "cross-env NODE_PATH=src:src/components",
    "start": "$npm_execpath run env:path react-scripts start",
    "build": "$npm_execpath run env:path react-scripts build",
    "test": "$npm_execpath run env:path react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "$npm_execpath run env:path start-storybook -p 9009 -s public",
    "build-storybook": "$npm_execpath run env:path build-storybook -s public",
    "postbuild": "./tools/moveBuildFolder.sh",
    "generate": "python tools/generate-component.py",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/core": "^1.3.8",
    "@date-io/date-fns": "^1.3.8",
    "@date-io/dayjs": "^1.3.8",
    "@date-io/luxon": "^1.3.8",
    "@date-io/moment": "^1.3.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.1.2",
    "animate.css": "^3.7.2",
    "axios": "^0.18.0",
    "bricks.js": "^1.8.0",
    "browserslist": "^4.6.4",
    "cross-env": "^5.2.0",
    "date-fns": "^2.0.0-beta.2",
    "dayjs": "^1.8.15",
    "i18next": "^15.1.3",
    "i18next-browser-languagedetector": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "luxon": "^1.16.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-infinite-scroller": "^1.2.4",
    "react-loadable": "^5.5.0",
    "react-md-spinner": "^0.3.0",
    "react-player": "^1.11.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-scripts": "3.0.0",
    "react-swipe": "^6.0.4",
    "redux": "^4.0.2",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable": "^4.0.0",
    "redux-pender": "^2.0.12",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "styled-theming": "^2.2.0",
    "swipe-js-iso": "^2.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.1.1",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-loader": "^8.0.5",
    "require-context.macro": "^1.0.4"
  },
  "license": "MIT"
}
